extends layouts/main

block content 
    ul 
        li Image Type 
    .image-style__container
        .image__view 
            image(src=`/${image.fileUrl}`)
        .image__info
            h5.image__style=image.style
        .colortable__info
            h6.colortable__color=colorTable.color 
            h6.colortable__colorcode=colorTable.colorCode
        .effecttable__info 
            h6.effecttable__positive="positive"+ effectTable.positive 
            h6.effecttable__negative="negative" + effectTable.negative 
            h6.effecttable__symbole="symbol" + effectTable.symbol 
            h6.effecttable__effect="effect" + effectTable.effect